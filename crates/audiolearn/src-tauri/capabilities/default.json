{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "AudioLearn default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
